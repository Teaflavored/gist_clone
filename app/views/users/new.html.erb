<form action="<%= users_url %>"
      method="POST">
  <input type="hidden"
         name="authenticity_token"
         value="<%= form_authenticity_token %>"
  <label for="user_username">Username</label>
  <input type="text"
         name="user[username]">
  <label for="user_password">Password</label>
  <input type="password"
         name="user[password]">

  <input type="submit" value="sign up">
</form>
